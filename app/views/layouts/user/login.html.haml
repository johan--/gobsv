!!! 5
%html
  %head
    %meta{ charset: 'utf-8' }
    %meta{ name: 'viewport', content: 'width=device-width, initial-scale=1, maximum-scale=1' }
    %meta{ name: 'robots', content: 'noindex' }
    %meta{ name: 'description', content: 'Conozca los empleos públicos disponibles en el gobierno' }
    %title
      Empleos públicos en El Salvador
    = display_meta_tags
    = stylesheet_link_tag 'user/login'
    = javascript_include_tag 'user/login'
    = csrf_meta_tags
  %body.animated.fadeInDown
    = render partial: 'layouts/user/header'
    = render partial: 'layouts/shared/notice'
    =# render partial: 'layouts/employments/breadcrumb'
    #wrapper
      .container
        = yield

    = render partial: 'layouts/employments/footer'
